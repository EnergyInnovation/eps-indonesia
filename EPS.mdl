	~		|

This Year CO2 Sequestered[electricity sector]=
	SUM(This Year Pollutants Sequestered by Electricity Sector[CO2,Electricity Source!])
	/ Grams per Metric Ton CO2e
	/ Grams Pollutant per Gram CO2e Unit Converter ~~|
This Year CO2 Sequestered[industry sector]=
	SUM(This Year Pollutants Sequestered by Industry Sector[Industry Category!,CO2])
	/ Grams per Metric Ton CO2e
	/ Grams Pollutant per Gram CO2e Unit Converter
	~	metric tons CO2e
	~		|

This Year Pollutants Sequestered by Electricity Sector[Pollutant,Electricity Source]=
	MIN(
	  Electricity Sector Pollutant Emissions before CCS[Pollutant,Electricity Source],
	
	  This Year Potential Pollutants Sequestered by Electricity Sector[Pollutant,Electricity Source\
		]
	)
	~	g pollutant
	~		|

This Year Pollutants Sequestered by Industry Sector[Industry Category,Pollutant]=
	MIN(
	  SUM(Industrial Sector Pollutant Emissions from Fuels for Energy Purposes before CCS\
		[Industrial Fuel!,Industry Category,Pollutant]),
	
	  This Year Potential Pollutants Sequestered by Industry Sector[Industry Category,Pollutant\
		]
	)
	~	g pollutant
	~		|

This Year Potential CO2 Sequestered[CCS Sectors]=
	(CSA BAU Tons CO2 Sequestered
	  + (CSA Additional CCS Potential
	     * Fraction of Potential Additional CCS Achieved This Year)
	)
	* CPbE Fraction of CO2 Sequestration by Sector[CCS Sectors]
	~	metric tons CO2e
	~		|

Electricity Sector Pollutant Emissions[Pollutant]=
	SUM(Electricity Sector Pollutant Emissions before CCS[Pollutant,Electricity Source!]\
		)
	+ SUM(Electricity Sector CCS Pollutant Emissions[Electricity Source!,Pollutant])
	- SUM(This Year Pollutants Sequestered by Electricity Sector[Pollutant,Electricity Source\
		!])
	~	g pollutant
	~		|

Electricity Sector Pollutant Emissions before CCS[Pollutant,Electricity Source]=
