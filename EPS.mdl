	)
	~	g pollutant
	~		|

Interim Period 2 Compliance=
	IF THEN ELSE(
	  Time = 2030 :AND: Electricity Sector Cumulative 2025 to 2027 Emissions[CO2]
	  < GET DATA AT TIME(CPPCS Clean Power Plan Compliance Schedule, 2027),
	
	  1,
	
	  0
	)
	~	Dmnl
	~		|

Electricity Sector Cumulative 2022 to 2024 Emissions[Pollutant]= INTEG (
	IF THEN ELSE(
	  Time > 2021 :AND: Time < 2025,
	  Electricity Sector Pollutant Emissions[Pollutant] / One Year,
	  0
	),
		0)
	~	g pollutant
	~		|

Electricity Sector Cumulative 2025 to 2027 Emissions[Pollutant]= INTEG (
	IF THEN ELSE(
	  Time > 2024 :AND: Time < 2028,
